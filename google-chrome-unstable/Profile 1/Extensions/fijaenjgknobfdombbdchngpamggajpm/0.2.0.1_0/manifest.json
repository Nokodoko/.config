{
   "background": {
      "scripts": [ "background.js" ]
   },
   "content_scripts": [ {
      "js": [ "num_tab.js" ],
      "matches": [ "*://*/*", "file://*/*" ],
      "run_at": "document_idle"
   } ],
   "description": "Number tabs when the ctrl/alt/cmd button is held.",
   "icons": {
      "128": "icon-128.gif",
      "48": "icon-48.gif"
   },
   "key": "",
